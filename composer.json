{
  "name": "haakco/postgres-helper",
  "description": ":package_description",
  "license": "proprietary",
  "authors": [
    {
      "name": "Tim Haak",
      "email": "tim@haak.co",
      "homepage": "https://haak.co"
    }
  ],
  "homepage": "https://github.com/haakco/postgres-helper",
  "keywords": [
    "Laravel",
    "PostgresHelper"
  ],
  "require": {
    "illuminate/support": "~7|~8"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "~9.0",
    "orchestra/testbench": "~5|~6",
    "brainmaestro/composer-git-hooks": "^2.8",
    "doctrine/dbal": "^2.13.2",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "HaakCo\\PostgresHelper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HaakCo\\PostgresHelper\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "HaakCo\\PostgresHelper\\PostgresHelperServiceProvider"
      ],
      "aliases": {
        "PostgresHelper": "HaakCo\\PostgresHelper\\Facades\\PostgresHelper"
      }
    }
  },
  "repositories": {
    "haakco/laravel-postgres-helper": {
      "type": "vcs",
      "url": "git@github.com:haakco/laravel-postgres-helper.git"
    }
  }
}
